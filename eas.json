{
  "cli": {
    "version": ">= 16.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "FIREBASE_API_KEY": "FIREBASE_API_KEY_PREVIEW",
        "FIREBASE_AUTH_DOMAIN": "FIREBASE_AUTH_DOMAIN_PREVIEW",
        "FIREBASE_PROJECT_ID": "FIREBASE_PROJECT_ID_PREVIEW",
        "FIREBASE_STORAGE_BUCKET": "FIREBASE_STORAGE_BUCKET_PREVIEW",
        "FIREBASE_MESSAGING_SENDER_ID": "FIREBASE_MESSAGING_SENDER_ID_PREVIEW",
        "FIREBASE_APP_ID": "FIREBASE_APP_ID_PREVIEW",
        "FIREBASE_MEASUREMENT_ID": "FIREBASE_MEASUREMENT_ID_PREVIEW"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "FIREBASE_API_KEY": "FIREBASE_API_KEY_PROD",
        "FIREBASE_AUTH_DOMAIN": "FIREBASE_AUTH_DOMAIN_PROD",
        "FIREBASE_PROJECT_ID": "FIREBASE_PROJECT_ID_PROD",
        "FIREBASE_STORAGE_BUCKET": "FIREBASE_STORAGE_BUCKET_PROD",
        "FIREBASE_MESSAGING_SENDER_ID": "FIREBASE_MESSAGING_SENDER_ID_PROD",
        "FIREBASE_APP_ID": "FIREBASE_APP_ID_PROD",
        "FIREBASE_MEASUREMENT_ID": "FIREBASE_MEASUREMENT_ID_PROD"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
